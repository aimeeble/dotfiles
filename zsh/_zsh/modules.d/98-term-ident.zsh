      IFS="" read -r -d '' TEXT << '__EOF'
               ___    __      _
              /   |  / /___  (_)___  ___
             / /| | / / __ \/ / __ \/ _ \
            / ___ |/ / /_/ / / / / /  __/
           /_/  |_/_/ .___/_/_/ /_/\___/
                   /_/
\033[8A
__EOF
      ;;

    Linux-arch)
