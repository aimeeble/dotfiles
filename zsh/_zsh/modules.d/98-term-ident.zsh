            / /___/ / / / / /_/ />  <
           /_____/_/_/ /_/\__,_/_/|_|
\033[7A
__EOF
      ;;

  esac

  if [[ -n "${IMGDATA}" ]]; then
    echo
    echo "${TEXT}"
    if [[ "${LC_TERMINAL:-}" == "iTerm2" && -z $(__iterm2_is_tmux) ]]; then
      __iterm2_print_image "" 1 "${IMGDATA}" 0 "" "5" "1" ""
    else
      echo; echo; echo; echo; echo
    fi
    echo
  fi
}

_aimee_termident_init() {
  preshell_functions=(_aimee_termident_shell $preshell_functions)
}

module_add "Term Ident" _aimee_termident_init
