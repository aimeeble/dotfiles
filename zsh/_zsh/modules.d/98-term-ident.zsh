      IMGDATA=""
      IFS="" read -r -d '' TEXT << '__EOF'
               ___              __
              /   |  __________/ /_
             / /| | / ___/ ___/ __ \
            / ___ |/ /  / /__/ / / /
           /_/  |_/_/   \___/_/ /_/
\033[7A
__EOF
      ;;

    Linux-debian)
      # src file: "distro-debian.png"
      IMGDATA=""
      IFS="" read -r -d '' TEXT << '__EOF'
               ____       __    _
              / __ \___  / /_  (_)___ _____
             / / / / _ \/ __ \/ / __ `/ __ \
            / /_/ /  __/ /_/ / / /_/ / / / /
           /_____/\___/_.___/_/\__,_/_/ /_/
\033[7A
__EOF
      ;;

    Linux-ubuntu)
      # src file: "distro-ubuntu.png"
      IMGDATA=""
      IFS="" read -r -d '' TEXT << '__EOF'
              __  ____                __
             / / / / /_  __  ______  / /___  __
            / / / / __ \/ / / / __ \/ __/ / / /
