
__iterm2_is_tmux() {
    [[ $TERM == screen* || $TERM == tmux* ]] && echo 1
}

__iterm2_print_osc() {
    printf "\033]"
}

__iterm2_print_st() {
    printf "\a"
}

__iterm2_print_image() {
    local B64VER="$(base64 --version 2>&1)"
    B64ARG="-D"
    if [[ "$B64VER" =~ "GNU" ]]; then
        B64ARG="-di"
    fi

    __iterm2_print_osc
    printf "1337;File=inline=%s" "$2"
    printf ";size=%d" $(printf "%s" "$3" | base64 $B64ARG | wc -c)
    [ -n "$1" ] && printf ";name=%s" "$(printf "%s" "$1" | b64_encode)"
    [ -n "$5" ] && printf ";width=%s" "$5"
    [ -n "$6" ] && printf ";height=%s" "$6"
    [ -n "$7" ] && printf ";preserveAspectRatio=%s" "$7"
    [ -n "$8" ] && printf ";type=%s" "$8"
    printf ":%s" "$3"
    __iterm2_print_st
    printf '\n'
}

_aimee_termident_shell() {
